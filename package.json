    "ember-cli-valid-component-name": "^1.0.0",
    "ember-cli-version-checker": "^2.1.2",
    "ember-router-generator": "^1.2.3",
    "exists-sync": "^0.1.0",
    "fs-extra": "^7.0.0",
    "inflection": "^1.12.0",
    "silent-error": "^1.1.0"
  },
  "devDependencies": {
    "@typed-ember/renovate-config": "1.2.0",
    "broccoli-asset-rev": "3.0.0",
